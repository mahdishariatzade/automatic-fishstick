[server]
bind_addr = "{{ bind_addr | default('0.0.0.0:8443') }}"
transport = "wss"
token = "{{ token | default('your_token') }}"
channel_size = {{ channel_size | default(2048) }}
keepalive_period = {{ keepalive_period | default(75) }}
nodelay = {{ nodelay | default(true) }}
tls_cert = "{{ tls_cert | default('/root/server.crt') }}"
tls_key = "{{ tls_key | default('/root/server.key') }}"
sniffer = {{ sniffer | default(false) }}
web_port = {{ web_port | default(2060) }}
sniffer_log = "{{ sniffer_log | default('/root/backhaul.json') }}"
log_level = "{{ log_level | default('info') }}"
ports = {{ ports | default([]) | tojson }} 