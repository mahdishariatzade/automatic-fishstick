[client]
remote_addr = "{{ backhaul_vars.wssmux.client.remote_addr | default('0.0.0.0:443') }}"
edge_ip = "{{ backhaul_vars.wssmux.client.edge_ip | default('') }}"
transport = "wssmux"
token = "{{ backhaul_vars.common.token | default('your_token') }}"
keepalive_period = {{ backhaul_vars.wssmux.client.keepalive_period | default(75) }}
dial_timeout = {{ backhaul_vars.wssmux.client.dial_timeout | default(10) }}
nodelay = {{ backhaul_vars.wssmux.client.nodelay | default(true) | string | lower }}
retry_interval = {{ backhaul_vars.wssmux.client.retry_interval | default(3) }}
connection_pool = {{ backhaul_vars.wssmux.client.connection_pool | default(50) }}
aggressive_pool = {{ backhaul_vars.wssmux.client.aggressive_pool | default(false) | string | lower }}
mux_version = {{ backhaul_vars.wssmux.client.mux_version | default(1) }}
mux_framesize = {{ backhaul_vars.wssmux.client.mux_framesize | default(32768) }}
mux_recievebuffer = {{ backhaul_vars.wssmux.client.mux_recievebuffer | default(8388608) }}
mux_streambuffer = {{ backhaul_vars.wssmux.client.mux_streambuffer | default(524288) }}
sniffer = {{ backhaul_vars.common.sniffer | default(false) | string | lower }}
web_port = {{ backhaul_vars.common.web_port | default(2060) }}
sniffer_log = "{{ backhaul_vars.common.sniffer_log | default('/root/backhaul.json') }}"
log_level = "{{ backhaul_vars.common.log_level | default('info') }}" 