[client]
remote_addr = "{{ backhaul_vars.ws.client.remote_addr | default('0.0.0.0:8080') }}"
edge_ip = "{{ backhaul_vars.ws.client.edge_ip | default('') }}"
transport = "ws"
token = "{{ backhaul_vars.common.token | default('your_token') }}"
connection_pool = {{ backhaul_vars.ws.client.connection_pool | default(8) }}
aggressive_pool = {{ backhaul_vars.ws.client.aggressive_pool | default(false) }}
keepalive_period = {{ backhaul_vars.ws.client.keepalive_period | default(75) }}
dial_timeout = {{ backhaul_vars.ws.client.dial_timeout | default(10) }}
retry_interval = {{ backhaul_vars.ws.client.retry_interval | default(3) }}
nodelay = {{ backhaul_vars.ws.client.nodelay | default(true) | string | lower }}
sniffer = {{ backhaul_vars.common.sniffer | default(false) | string | lower }}
web_port = {{ backhaul_vars.common.web_port | default(2060) }}
sniffer_log = "{{ backhaul_vars.common.sniffer_log | default('/root/backhaul.json') }}"
log_level = "{{ backhaul_vars.common.log_level | default('info') }}" 