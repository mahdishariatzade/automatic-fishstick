[client]
remote_addr = "{{ backhaul_vars.udp.client.remote_addr | default('0.0.0.0:3080') }}"
transport = "udp"
token = "{{ backhaul_vars.common.token | default('your_token') }}"
connection_pool = {{ backhaul_vars.udp.client.connection_pool | default(8) }}
aggressive_pool = {{ backhaul_vars.udp.client.aggressive_pool | default(false) }}
retry_interval = {{ backhaul_vars.udp.client.retry_interval | default(3) }}
sniffer = {{ backhaul_vars.common.sniffer | default(false) | string | lower }}
web_port = {{ backhaul_vars.common.web_port | default(2060) }}
sniffer_log = "{{ backhaul_vars.common.sniffer_log | default('/root/backhaul.json') }}"
log_level = "{{ backhaul_vars.common.log_level | default('info') }}" 