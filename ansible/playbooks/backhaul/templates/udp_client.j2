[client]
remote_addr = "{{ remote_addr | default('0.0.0.0:3080') }}"
transport = "udp"
token = "{{ token | default('your_token') }}"
connection_pool = {{ connection_pool | default(8) }}
aggressive_pool = {{ aggressive_pool | default(false) }}
retry_interval = {{ retry_interval | default(3) }}
sniffer = {{ sniffer | default(false) }}
web_port = {{ web_port | default(2060) }}
sniffer_log = "{{ sniffer_log | default('/root/backhaul.json') }}"
log_level = "{{ log_level | default('info') }}" 