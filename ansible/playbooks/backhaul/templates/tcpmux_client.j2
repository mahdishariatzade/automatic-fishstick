[client]
remote_addr = "{{ backhaul_vars.client_common.remote_addr | default('0.0.0.0:3080') }}"
transport = "tcpmux"
token = "{{ backhaul_vars.common.token | default('your_token') }}"
connection_pool = {{ backhaul_vars.client_common.connection_pool | default(50) }}
aggressive_pool = {{ backhaul_vars.client_common.aggressive_pool | default(false) }}
keepalive_period = {{ backhaul_vars.client_common.keepalive_period | default(75) }}
dial_timeout = {{ backhaul_vars.client_common.dial_timeout | default(10) }}
retry_interval = {{ backhaul_vars.client_common.retry_interval | default(3) }}
nodelay = {{ backhaul_vars.client_common.nodelay | default(true) }}
mux_version = {{ backhaul_vars.tcpmux.client.mux_version | default(1) }}
mux_framesize = {{ backhaul_vars.tcpmux.client.mux_framesize | default(32768) }}
mux_recievebuffer = {{ backhaul_vars.tcpmux.client.mux_recievebuffer | default(8388608) }}
mux_streambuffer = {{ backhaul_vars.tcpmux.client.mux_streambuffer | default(524288) }}
sniffer = {{ backhaul_vars.common.sniffer | default(false) }}
web_port = {{ backhaul_vars.common.web_port | default(2060) }}
sniffer_log = "{{ backhaul_vars.common.sniffer_log | default('/root/backhaul.json') }}"
log_level = "{{ backhaul_vars.common.log_level | default('info') }}" 