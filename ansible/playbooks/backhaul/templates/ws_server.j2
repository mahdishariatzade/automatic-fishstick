[server]
bind_addr = "{{ backhaul_vars.ws.server.bind_addr | default('0.0.0.0:8080') }}"
transport = "ws"
token = "{{ backhaul_vars.common.token | default('your_token') }}"
channel_size = {{ backhaul_vars.ws.server.channel_size | default(2048) }}
keepalive_period = {{ backhaul_vars.ws.server.keepalive_period | default(75) }}
heartbeat = {{ backhaul_vars.ws.server.heartbeat | default(40) }}
nodelay = {{ backhaul_vars.ws.server.nodelay | default(true) | string | lower }}
sniffer = {{ backhaul_vars.common.sniffer | default(false) | string | lower }}
web_port = {{ backhaul_vars.common.web_port | default(2060) }}
sniffer_log = "{{ backhaul_vars.common.sniffer_log | default('/root/backhaul.json') }}"
log_level = "{{ backhaul_vars.common.log_level | default('info') }}"
ports = {{ backhaul_vars.ws.server.ports | default([]) | tojson }} 