[server]
bind_addr = "{{ backhaul_vars.wsmux.server.bind_addr | default('0.0.0.0:3080') }}"
transport = "wsmux"
token = "{{ backhaul_vars.common.token | default('your_token') }}"
keepalive_period = {{ backhaul_vars.wsmux.server.keepalive_period | default(75) }}
nodelay = {{ backhaul_vars.wsmux.server.nodelay | default(true) | string | lower }}
heartbeat = {{ backhaul_vars.wsmux.server.heartbeat | default(40) }}
channel_size = {{ backhaul_vars.wsmux.server.channel_size | default(1024) }}
mux_con = {{ backhaul_vars.wsmux.server.mux_con | default(20) }}
mux_version = {{ backhaul_vars.wsmux.server.mux_version | default(1) }}
mux_framesize = {{ backhaul_vars.wsmux.server.mux_framesize | default(32768) }}
mux_recievebuffer = {{ backhaul_vars.wsmux.server.mux_recievebuffer | default(8388608) }}
mux_streambuffer = {{ backhaul_vars.wsmux.server.mux_streambuffer | default(524288) }}
sniffer = {{ backhaul_vars.common.sniffer | default(false) }}
web_port = {{ backhaul_vars.common.web_port | default(2060) }}
sniffer_log = "{{ backhaul_vars.common.sniffer_log | default('/root/backhaul.json') }}"
log_level = "{{ backhaul_vars.common.log_level | default('info') }}"
ports = {{ backhaul_vars.wsmux.server.ports | default([]) | tojson }} 